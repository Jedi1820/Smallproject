<!--pages/4.29/one.wxml-->
<view class='base'>
   <view class='one'>
    <block wx:for="{{videodata}}">
      <video class='vdview' controls='controls' src='{{item.videopath}}'></video>
      <view class='text'>
        <view class='text1'>
           <text>{{item.name}}</text>
        </view>
        <view class='text2'>
           <text>{{item.author}}</text>
        </view>
      </view>
    </block>
   </view>
</view>